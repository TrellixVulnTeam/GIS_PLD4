import urllib
import urllib.request
params = {'xmin': -6.185440796831979, 'ymin': 106.82374835014343,'xmax': -6.178966266481431, 'ymax': 106.83127999305725, 'paranthesis[': '[', 'paranthesis]': ']'}
myOsmXmlUrlPath = ('http://www.overpass-api.de/api/xapi?node[bbox=7.1,51.2,7.2,51.3][highway=*]' % params)
# Note that osm json is NOT geojson!

try:
    myUrlHandle = urllib.request.urlopen(myOsmXmlUrlPath, timeout=60)
    myFile = file('osm.xml', 'wb')
    myFile.write(myUrlHandle.read())
    myFile.close()
except: print('no')
